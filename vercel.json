{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    { "path": "/api/bing/sync/simple", "schedule": "*/1 * * * *" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
